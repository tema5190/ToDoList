@model IEnumerable<ModelsAndServices.Models.Task>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
     @Html.ActionLink("Create New", "Create", "Task", new {@class = "col-large-12 btn btn-success btn-block add-task-button" })
</div>


@foreach (var item in Model)
{
    <div class="jumbotron task-info-container">
        @Html.Partial("TaskInfo", item);
    </div>
}


